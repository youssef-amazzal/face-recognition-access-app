<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import org.kordamp.ikonli.javafx.FontIcon?>

<HBox fx:id="root" alignment="CENTER_LEFT" maxWidth="1.7976931348623157E308" prefHeight="0.0" spacing="10.0" styleClass="step-indicator" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox alignment="CENTER" prefHeight="64.0">
         <children>
            <FontIcon fx:id="StepIcon" iconLiteral="mdi2f-file-document" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
      <VBox alignment="CENTER_LEFT" layoutX="10.0" layoutY="10.0" spacing="5.0" HBox.hgrow="ALWAYS">
         <children>
            <Label fx:id="StepNumber" text="Step 1">
               <styleClass>
                  <String fx:value="text-bold" />
                  <String fx:value="text-subtle" />
                  <String fx:value="step-number" />
               </styleClass>
            </Label>
            <Label fx:id="StepTitle" text="General Information">
               <styleClass>
                  <String fx:value="text-bold" />
                  <String fx:value="step-title" />
               </styleClass></Label>
         </children>
      </VBox>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</HBox>
