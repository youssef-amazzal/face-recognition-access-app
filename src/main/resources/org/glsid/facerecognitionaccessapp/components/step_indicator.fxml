<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import org.kordamp.ikonli.javafx.FontIcon?>

<ToggleButton fx:id="root" maxWidth="1.7976931348623157E308" mnemonicParsing="false" prefHeight="80.0" styleClass="step-panel" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1">
   <graphic>
      <HBox alignment="CENTER_LEFT" prefWidth="200.0" spacing="10.0" styleClass="abbr">
         <children>
            <VBox alignment="CENTER" prefHeight="64.0">
               <children>
                  <FontIcon fx:id="StepIcon" iconLiteral="mdi2f-file-document" />
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </VBox>
            <VBox alignment="CENTER_LEFT" layoutX="10.0" layoutY="10.0" HBox.hgrow="ALWAYS">
               <children>
                  <Label fx:id="StepNumber" text="Step 1">
                     <styleClass>
                        <String fx:value="text-bold" />
                        <String fx:value="text-subtle" />
                     </styleClass>
                  </Label>
                  <Label fx:id="StepTitle" styleClass="text-bold" text="General Information" />
               </children>
            </VBox>
         </children>
      </HBox>
   </graphic>
</ToggleButton>
