<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>


<StackPane fx:id="root" alignment="BOTTOM_RIGHT" prefHeight="900.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.glsid.facerecognitionaccessapp.presentation.views.FaceCamViewController">
   <children>
      <ImageView fx:id="FrameView" fitHeight="900.0" fitWidth="1280.0" pickOnBounds="true" preserveRatio="true" />
      <HBox fx:id="CapturedImageSlot" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="160.0" prefWidth="160.0">
         <children>
            <ImageView fx:id="CapturedImage" fitHeight="160.0" fitWidth="160.0" pickOnBounds="true" preserveRatio="true" />
         </children>
         <StackPane.margin>
            <Insets bottom="50.0" right="50.0" />
         </StackPane.margin>
         <styleClass>
            <String fx:value="my-card" />
            <String fx:value="outlined" />
            <String fx:value="rounded" />
         </styleClass>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </HBox>
   </children>
</StackPane>
