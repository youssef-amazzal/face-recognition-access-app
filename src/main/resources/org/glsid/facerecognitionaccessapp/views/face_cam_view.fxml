<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane fx:id="root" prefHeight="900.0" prefWidth="1280.0" styleClass="bg" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.glsid.facerecognitionaccessapp.presentation.views.FaceCamViewController">
   <children>
      <ImageView fx:id="FrameView" fitHeight="900.0" fitWidth="1280.0" pickOnBounds="true" preserveRatio="true" />
      <VBox fx:id="CapturedImages" alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="110.0" prefWidth="110.0" spacing="10.0" StackPane.alignment="CENTER_RIGHT">
         <styleClass>
            <String fx:value="outlined" />
            <String fx:value="my-card" />
            <String fx:value="rounded" />
         </styleClass>
         <StackPane.margin>
            <Insets right="40.0" />
         </StackPane.margin>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
      <Button fx:id="CaptureButton" disable="true" mnemonicParsing="false" prefHeight="69.0" prefWidth="191.0" text="Capture" StackPane.alignment="BOTTOM_CENTER">
         <StackPane.margin>
            <Insets bottom="50.0" />
         </StackPane.margin>
         <styleClass>
            <String fx:value="accent" />
            <String fx:value="title-3" />
            <String fx:value="text-bold" />
         </styleClass>
      </Button>
   </children>
</StackPane>
